{
  "name": "KetoWP",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Adun is a simple and secure WordPress plugin that houses our Agency's in-house custom design (powered by our popular #1 variable-first atomic design framework - KetoCSS). It also has all our pre and post-deployment snippets.",
  "main": "index.js",
  "scripts": {
    "dev": "postcss src/styles/main.css -o assets/css/style.css --watch",
    "build": "postcss src/styles/main.css -o assets/css/style.css --env production",
    "start": "npm run dev"
  },
  "keywords": [
    "ketoCSS",
    "adun.studio",
    "bricks builder",
    "bricks child theme",
    "sass",
    "ecommerce",
    "modern css",
    "responsive design",
    "postcss-purgecss",
    "sass-starter-template",
    "postcss",
    "postcss-sass",
    "cssnano",
    "postcss-preset-env",
    "grid",
    "flexbox"
  ],
  "author": "Claudius A.",
  "license": "ISC",
  "browserslist": [
    ">0.2%",
    "last 2 versions",
    "not dead"
  ],
  "devDependencies": {
    "@csstools/postcss-sass": "^5.0.1",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@prettier/plugin-php": "^0.20.1",
    "autoprefixer": "^10.4.21",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.5.6",
    "postcss-cli": "^10.1.0",
    "postcss-preset-env": "^9.1.3",
    "tailwindcss": "^3.3.6"
  },
  "dependencies": {
    "alpinejs": "^3.13.0"
  }
}
